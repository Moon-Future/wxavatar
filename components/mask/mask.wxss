.mask-container {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 400rpx;
  background: rgba(255, 255, 255, 0.8);
}

.mask-header {
  background: rgba(200, 200, 200, 0.8);
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 10rpx;
}

.mask-header .nav .cu-item {
  height: initial;
  line-height: initial;
  padding: 10rpx;
}

.mask-body {
  display: flex;
  flex-wrap: wrap;
  justify-content: start;
  height: 340rpx;
  overflow: auto;
}

.mask-image {
  width: 150rpx;
  height: 150rpx;
  padding: 10rpx;
}

.mask-body .cu-load.over::after {
	content: "暂无数据";
}